{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "google/recaptcha": "1.2.4",
        "karser/karser-recaptcha3-bundle": "0.1.18",
        "sensio/framework-extra-bundle": "6.1.5",
        "symfony/asset": "5.3.0",
        "symfony/console": "5.3.0",
        "symfony/dotenv": "5.3.0",
        "symfony/expression-language": "5.3.0",
        "symfony/flex": "1.13.3",
        "symfony/form": "5.3.0",
        "symfony/framework-bundle": "5.3.0",
        "symfony/http-client": "5.3.0",
        "symfony/intl": "5.3.0",
        "symfony/mailer": "5.3.0",
        "symfony/monolog-bundle": "3.7.0",
        "symfony/notifier": "5.3.0",
        "symfony/orm-pack": "2.1.0",
        "symfony/process": "5.3.0",
        "symfony/security-bundle": "5.3.0",
        "symfony/serializer-pack": "1.0.4",
        "symfony/string": "5.3.0",
        "symfony/swiftmailer-bundle": "3.5.2",
        "symfony/translation": "5.3.0",
        "symfony/twig-pack": "1.0.1",
        "symfony/validator": "5.3.1",
        "symfony/web-link": "5.3.0",
        "symfony/yaml": "5.3.0"
    },
    "require-dev": {
        "symfony/debug-pack": "1.0.9",
        "symfony/maker-bundle": "1.31.1",
        "symfony/profiler-pack": "1.0.5",
        "symfony/test-pack": "1.0.8"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.*"
        }
    }
}
